<%= CurationConcerns::Renderers::AttributeRenderer.new("Created Date", date_formatter( presenter.solr_document.date_created ), {include_empty: true}).render %>
<%= presenter.attribute_to_html(:rights, render_as: :rights) %>
<%= show_line(presenter, :audit_history, 'Audit', true, include_empty: false, catalog_search_link: false ) if @current_user.admin? %>

